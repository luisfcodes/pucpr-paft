    "x = 1\n",
    "n = int(input(\"Digite um número natural > 0 e <= 100 para iniciar a sequência: \"))\n",
    "print(\"\")\n",
    "\n",
    "if n <= 100 and n > 0:\n",
    "    while x <= n:\n",
    "        for i in range(1, x + 1):\n",
    "            print(x, end=\" \")\n",
    "        print(\" \")\n",
    "        x = x + 1\n",
    "else:\n",
    "    print(\"Entrada inválida.\")"
